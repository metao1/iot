{
    "name": "persoinfo",
    "version": "6.2.4",
    "license": "http://persoinfo.com/licenses/terms/regular",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
        "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
        "build": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev",
        "build-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev --stats-json",
        "build-prod": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod",
        "build-prod-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "bundle-report": "webpack-bundle-analyzer persoinfo.github.io/stats.json",
        "deploy": "ng build --prod --base-href https://persoinfo.github.io/persoinfo/persoinfo.github.io",
        "push": "ngh --dir persoinfo.github.io"
    },
    "private": true,
    "dependencies": {
        "@angular-builders/custom-webpack": "8.4.1",
        "@angular/animations": "8.2.5",
        "@angular/cdk": "8.2.0",
        "@angular/cli": "8.3.4",
        "@angular/common": "8.2.5",
        "@angular/compiler": "8.2.5",
        "@angular/core": "8.2.5",
        "@angular/flex-layout": "8.0.0-beta.27",
        "@angular/forms": "8.2.5",
        "@angular/material": "8.2.0",
        "@angular/material-moment-adapter": "^8.2.3",
        "@angular/platform-browser": "8.2.5",
        "@angular/platform-browser-dynamic": "8.2.5",
        "@angular/router": "8.2.5",
        "@ng-bootstrap/ng-bootstrap": "^5.1.4",
        "@ngrx/effects": "^8.6.0",
        "@ngrx/router-store": "8.6.0",
        "@ngrx/store": "8.6.0",
        "@ngrx/store-devtools": "^8.6.0",
        "@ngx-translate/core": "^11.0.1",
        "angular2-ui-switch": "^1.2.0",
        "chart.js": "^2.9.3",
        "classlist.js": "1.1.20150312",
        "keycloak-angular": "^7.0.1",
        "keycloak-js": "^6.0.1",
        "moment": "^2.24.0",
        "ng2-charts": "^2.3.0",
        "ng2-file-upload": "^1.4.0",
        "ngx-cookie-service": "^2.2.0",
        "ngx-webstorage": "^4.0.1",
        "rxjs": "^6.5.3",
        "web-animations-js": "^2.3.1",
        "zone.js": "^0.8.29",
        "core-js": "2.5.7"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.803.21",
        "@angular/cli": "8.3.4",
        "@angular/compiler-cli": "8.2.5",
        "@angular/language-service": "8.2.5",
        "@angularclass/hmr": "2.1.3",
        "@ngrx/schematics": "8.3.0",
        "@types/jasmine": "3.3.8",
        "@types/jasminewd2": "2.0.3",
        "@types/node": "8.9.4",
        "ngrx-store-freeze": "^0.2.4",
        "protractor": "^6.0.0",
        "typescript": "3.4.2"
    }
}
